{
    "manifest_version": 3,
    "name": "instant_translate",
    "description": "Instant Translation Extension",
    "version": "1.0",
    "permissions": [
    "clipboardRead","clipboardWrite","storage"],
    "web_accessible_resources": [{
      "resources":["Assets/ssMaCVm.jpg","Assets/birb.jpg"],
      "matches": ["https://web.whatsapp.com/*"]
    }],
    "action": {
      "default_popup": "src/index.html",
      "default_icon": "Assets/logo.png"
      },
    "content_scripts":[{
      "js":["scripts/content_script_1.js"],
      "matches":["https://www.youtube.com/*","https://www.amazon.in/*","https://www.google.com/*","https://web.whatsapp.com/*"]
    }]
     
  }